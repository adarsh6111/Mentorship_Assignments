
#include<bits/stdc++.h>
using namespace std;
int main(){
    int even=0,odd=0,n;
    cin>>n;
    for(int i =0;i<=n;i++)
    {
        if(i%2==0)
        even=even+i;
        else 
        odd=odd+i;
    }
    cout<<"sum of even is "<<even<<endl;
    cout<<"sum of oddd is "<<odd;
    return 0;

}


